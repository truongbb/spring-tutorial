<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">


    <bean id="gender" class="vn.com.ntqsolution.bean.common.Gender">
        <property name="id" value="1"/>
        <property name="gender" value="Nam"/>
    </bean>

    <bean id="person" class="vn.com.ntqsolution.bean.common.Person" autowire="byName">
        <property name="id" value="1"/>
        <property name="name" value="Người"/>
        <property name="phone" value="0912345678"/>
    </bean>


</beans>